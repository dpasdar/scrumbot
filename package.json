{
  "name": "scrumbot",
  "version": "1.0.0",
  "description": "scrumbot",
  "main": "index.js",
    "scripts": {
        "build": "tsc --build",
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "postinstall": "npm run build && node ./deploymentScripts/webConfigPrep.js",
        "start": "tsc --build && node ./lib/index.js",
        "test": "tsc --build && nyc mocha lib/tests/**/*.test.js",
        "watch": "nodemon --watch ./src -e ts --exec \"npm run start\""
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "**/.eslintrc.js",
            "**/*.d.ts",
            "**/*.test.*",
            "**/tests",
            "**/coverage",
            "**/deploymentScripts",
            "**/src/index.ts"
        ],
        "reporter": [
            "text"
        ],
        "all": true
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szul/chatbot-edui2018.git"
  },
  "keywords": [
    "chatbot",
    "bots",
    "bot",
    "framework",
    "edui"
  ],
  "author": "dpasdar <michael@szul.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szul/chatbot-edui2018/issues"
  },
  "homepage": "https://github.com/szul/chatbot-edui2018#readme",
  "dependencies": {
    "botbuilder": "^4.5.3",
    "botbuilder-ai": "^4.5.3",
    "botbuilder-azure": "^4.18.0",
    "botbuilder-dialogs": "^4.5.3",
    "botframework-config": "^4.5.3",
    "moment": "^2.29.2",
    "dotenv": "^8.0.0",
    "restify": "~8.3.3",
    "request-promise-native": "~1.0.7"
  },
  "devDependencies": {
        "@types/dotenv": "6.1.1",
        "@types/mocha": "^5.2.7",
        "@types/restify": "7.2.12",
        "mocha": "^10.2.0",
        "nodemon": "~1.19.1",
        "nyc": "^14.1.1",
        "ts-node": "^8.3.0",
        "tslint": "~5.18.0",
        "typescript": "~3.5.3"
    }
}
